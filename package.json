{
  "name": "mern-stack",
  "version": "1.0.0",
  "description": "Follow up MEARN Stack tutorial from Brad Traversy",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix",
    "server": "nodemon server.js",
    "start": "node server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bobslegend61/MERN-Stack.git"
  },
  "author": "Alabi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bobslegend61/MERN-Stack/issues"
  },
  "homepage": "https://github.com/Bobslegend61/MERN-Stack#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "express": "^4.16.3",
    "mongoose": "^5.1.7"
  }
}
